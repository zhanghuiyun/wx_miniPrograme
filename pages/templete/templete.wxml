            <view class="myCart">我的篮子</view>
            <view class="clean-cart">
                <view class="clear inline_block">清空篮子</view>
                <image class="deleteCart" src="http://sandboxuuhuiv2.pynoo.cn/Application/Home/View/WapSchoolMarket/source/css/images/delete.png" alt="" />
            </view>
        </view>
        <view class="am-cartListStyle" stock="91">
            <view class="am-cartList-title">娃娃娃娃娃娃娃娃娃娃娃娃娃娃娃娃娃娃娃娃娃娃娃娃娃娃娃娃</view>
            <view class="am-float-rt">
                <view class="am-price" g_name="娃娃" data-price="1.00" gid="303">￥1111.00</view>
                <image id="minus" src="http://sandboxuuhuiv2.pynoo.cn/Application/Home/View/WapSchoolMarket/source/css/images/minus.png" alt="" />
                <input type="number" class="am-count" confirm-type="done" disabled value="300"/>
                <image id="plus" src="http://sandboxuuhuiv2.pynoo.cn/Application/Home/View/WapSchoolMarket/source/css/images/plus.png" alt="" />
            </view>
        </view>
    </view>
</template>

