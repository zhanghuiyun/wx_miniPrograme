<!-- 搜索框 -->
<template name="searchBanner">
    <view class="searchBanner">
        <view class="selectSchool">
            <view class="location" style="background: url(http://sandboxuuhuiv2.pynoo.cn/Application/Home/View/WapSchoolMarket/source/css/images/pre.png) no-repeat;background-size: contain;background-position: 50%;"></view>
        </view>
        <view class="input">
            <form id="search_form">
                <input type="text" confirm-type="search" value="" id="search" />
            </form>
            <view class="palch">
                <view class="search_icon" style="background: url(http://sandboxuuhuiv2.pynoo.cn/Application/Home/View/WapSchoolMarket/source/css/images/search.png) no-repeat;background-size: contain;"></view>
                <view class="searchText">搜索校内购商品</view>
            </view>
        </view>
    </view>
</template>


<!-- 页脚购买 -->
<template name="settlement">
    <view id="J_settlement" class="settlement">

        <view class="settle_num">
            <image style="width:25px;" src="http://sandboxuuhuiv2.pynoo.cn/Application/Home/View/WapSchoolMarket/source/css/images/cart.png" alt="" />
            <view id="J_SelectedItemsCount" class="inline_block" style="display: inline;">83</view>
            <view id="J_totalPrice" class="settle_price inline_block">￥10849.04</view>
        </view>
        <button style="height: 48px;padding: 0;text-align: center;line-height: 48px;" id="J_settleBtn" class="btn">选好了</button>
    </view>
</template>

<!-- 购物车项列表 -->
